{
  "name": "greg-md/php-app-orm",
  "type": "library",
  "description": "Integration of Greg PHP ORM in Greg PHP Application.",
  "keywords": [ "greg-md", "greg-php", "php-app-orm", "greg-app-orm", "app-orm", "cache", "php", "web-artisans" ],
  "homepage": "https://github.com/greg-md/php-app-orm",
  "license": "MIT",
  "authors": [
    { "name": "Grigorii Duca", "email": "contact@greg.md", "homepage": "http://greg.md/" }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "ext-pdo": "*",
    "greg-md/php-app-installer": "dev-master",
    "greg-md/php-orm": "dev-master",
    "robmorgan/phinx": "^0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/var-dumper": "^4"
  },
  "autoload": {
    "psr-4": {
      "Greg\\AppOrm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greg\\AppOrm\\": "tests/"
    }
  }
}
